<?xml version="1.0" encoding="utf-8" ?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>OpenZiti.NET</id>
        <version>0.0.1</version>
        <title>Native Ziti Libraries for use with .NET</title>
        <authors>openziti.org</authors>
        <owners>OpenZiti Maintainers</owners>
        <license type="expression">Apache-2.0</license>
        <projectUrl>https://openziti.github.io/ziti-sdk-csharp</projectUrl>
        <icon>images\ziti-v2.png</icon>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <!--<summary1>A nuget package which wraps the native ziti C SDK into a package easily consumable from .NET</summary1>-->
        <summary>A .NET standard library which wraps the C SDK and is consumable from .NET</summary>
        <copyright>Copyright NetFoundry Inc. All rights reserved.</copyright>
        <tags>.net dotnet dll</tags>
        <description>This is the .NET library used to create applications which leverage the fully zero trust OpenZiti overlay network</description>
        <dependencies>
            <group targetFramework="net6.0">
                <dependency id="NLog" version="5.0.4" exclude="Build,Analyzers" />
                <dependency id="Newtonsoft.Json" version="13.0.1" exclude="Build,Analyzers" />
                <dependency id="System.Memory" version="4.5.5" exclude="Build,Analyzers" />
                <dependency id="System.Text.Json" version="6.0.6" exclude="Build,Analyzers" />
				<dependency id="OpenZiti.NET.native" version="0.30.8.132" exclude="Build,Analyzers" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="ziti-v2.png" target="images"/>
        <file src="bin\x86\Release\net6.0\OpenZiti.NET.dll" target="runtimes/win-x86/lib/net6.0" />
        <file src="bin\x86\Release\net6.0\OpenZiti.NET.pdb" target="runtimes/win-x86/lib/net6.0" />
        <file src="bin\x64\Release\net6.0\OpenZiti.NET.dll" target="runtimes/win-x64/lib/net6.0" />
        <file src="bin\x64\Release\net6.0\OpenZiti.NET.pdb" target="runtimes/win-x64/lib/net6.0" />
        <file src="bin\Release\net6.0\OpenZiti.NET.dll" target="lib/net6.0" />
        <file src="bin\Release\net6.0\OpenZiti.NET.pdb" target="lib/net6.0" />
    </files>
</package>

